{
  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "manifest_version": 2,
  "version": "2.0",
  "browser_action": {
    "default_title": "Make this page red",
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
